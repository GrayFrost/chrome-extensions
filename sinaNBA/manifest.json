{
	"manifest_version":2,
	"name":"sinaNBA",
	"version":"1.0",
	"description":"显示新浪nba的简短新闻",
	"background":{
		"scripts":[
		"js/jquery-1.4.2.min.js","js/background.js"]
	},
	"icons":{
		"48":"images/icon48.png"
	},
	"browser_action":{
		"default_icon":"images/icon128.png",
		"default_popup":"popup.html"
	},
	"permissions":[
		"*://sports.sina.com.cn/nba/*",
		"notifications"
	]
}